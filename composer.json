{
    "name": "outreach/filament-yoast-seo",
    "description": "Handle Yoast Seo for Filament admin panel",
    "keywords": [
        "outreach",
        "laravel",
        "filament-yoast-seo"
    ],
    "homepage": "https://github.com/outreach/filament-yoast-seo",
    "support": {
        "issues": "https://github.com/outreach/filament-yoast-seo/issues",
        "source": "https://github.com/outreach/filament-yoast-seo"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Outreach",
            "email": "kkelley@outreach.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/forms": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Outreach\\Filament\\FilamentYoastSeo\\": "src/",
            "Outreach\\Filament\\FilamentYoastSeo\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Outreach\\Filament\\FilamentYoastSeo\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Outreach\\Filament\\FilamentYoastSeo\\FilamentYoastSeoServiceProvider"
            ],
            "aliases": {
                "FilamentYoastSeo": "Outreach\\Filament\\FilamentYoastSeo\\Facades\\FilamentYoastSeo"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}