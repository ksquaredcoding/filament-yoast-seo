{
    "name": "outreach/filament-yoast-seo",
    "description": "Handle Yoast Seo for Filament admin panel",
    "keywords": [
        "outreach",
        "laravel",
        "filament-yoast-seo"
    ],
    "homepage": "https://github.com/outreach/filament-yoast-seo",
    "support": {
        "issues": "https://github.com/outreach/filament-yoast-seo/issues",
        "source": "https://github.com/outreach/filament-yoast-seo"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Outreach",
            "email": "kkelley@outreach.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/forms": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.9"
    },
    "autoload": {
        "psr-4": {
            "Outreach\\Filament\\FilamentYoastSeo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Outreach\\Filament\\FilamentYoastSeo\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Outreach\\Filament\\FilamentYoastSeo\\FilamentYoastSeoServiceProvider"
            ],
            "aliases": {
                "FilamentYoastSeo": "Outreach\\Filament\\FilamentYoastSeo\\Facades\\FilamentYoastSeo"
            }
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}